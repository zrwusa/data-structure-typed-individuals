{
  "name": "min-heap-typed",
  "version": "2.1.2",
  "description": "Min Heap",
  "browser": "dist/umd/max-heap-typed.min.js",
  "umd:main": "dist/umd/max-heap-typed.min.js",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=12.20.0",
    "npm": ">=6.14.0"
  },
  "scripts": {
    "build": "npm run build:ecu",
    "build:node": "tsup --config tsup.node.config.js",
    "build:types": "rm -rf dist/types && tsc -p tsconfig.types.json",
    "build:umd": "tsup",
    "build:ecu": "npm run build:node && npm run build:types && npm run build:umd",
    "lint": "eslint --fix \"src/**/*.{js,ts}\"",
    "format": "prettier --write \"src/**/*.{js,ts}\"",
    "test": "jest",
    "build:docs": "typedoc --out docs ./src",
    "deps:check": "dependency-cruiser src",
    "build:publish": "npm run build && npm publish"
  },
